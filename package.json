{
  "name": "nodejs-microservicios",
  "version": "1.0.0",
  "description": "Template para microservicios en NodeJS",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "migrate:generate": "sequelize-cli migration:generate --name",
    "migrate:run": "sequelize-cli db:migrate",
    "migrate:revert": "sequelize-cli db:migrate:undo",
    "migrate:delete": "sequelize-cli db:migrate:undo:all"
  },
  "keywords": [],
  "author": "Nahuel Zalazar Villca",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^2.1.7",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.4",
    "sequelize-cli": "^6.5.2",
    "swagger-ui-express": "^4.5.0"
  }
}
